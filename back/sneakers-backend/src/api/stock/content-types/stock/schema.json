{
  "kind": "collectionType",
  "collectionName": "stocks",
  "info": {
    "singularName": "stock",
    "pluralName": "stocks",
    "displayName": "stock"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "stocks"
    },
    "size": {
      "type": "string",
      "required": true
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "default": 0,
      "min": 0
    },
    "reserved_quantity": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "low_stock_threshold": {
      "type": "integer",
      "required": false,
      "default": 5
    },
    "price_adjustment": {
      "type": "decimal",
      "default": 0
    }
  }
}
